<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>com.pucpr.gss.GSS CadastroService</servlet-name>
        <servlet-class>br.pucpr.gss.server.service.CadastroServiceImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>br.pucpr.gss.GSS LoginService</servlet-name>
        <servlet-class>br.pucpr.gss.server.service.LoginServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.pucpr.gss.GSS CadastroService</servlet-name>
        <url-pattern>/GSS/Cadastro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>br.pucpr.gss.GSS LoginService</servlet-name>
        <url-pattern>/GSS/Login</url-pattern>
    </servlet-mapping>
</web-app>