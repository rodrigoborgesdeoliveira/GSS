<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://www.gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="GSS">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='br.pucpr.gss.client.GSS'/>

    <source path="client"/>
    <source path="shared"/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/Cadastro' class='br.pucpr.gss.server.service.CadastroServiceImpl'/>
    <servlet path="/Login" class="br.pucpr.gss.server.service.LoginServiceImpl"/>

</module>