<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://www.gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="GSS">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.pucpr.gss.client.controller.LoginController'/>

    <source path="client"/>
    <source path="server"/>
    <source path="shared"/>

    <!-- Specify the app servlets.                   -->
    <servlet path='/MySampleApplicationService'
             class='com.pucpr.gss.server.serviceImpl.MySampleApplicationServiceImpl'/>

</module>